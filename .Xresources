!! XFT
Xft.dpi:        96
Xft.rgba:       none
Xft.autohint:   false
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting:    true
Xft.antialias:  true

!! Urgency
URxvt.urgentOnBell: true

!! urxvt
! fonts
URxvt.font:xft:Hack-Regular:pixelsize=14, xft:Symbola
URxvt.boldFont:xft:Hack-Bold:pixelsize=14:weight=bold, xft:Symbola
URxvt.italicFont:xft:Hack-Italic:pixelsize=14:slant=italic, xft:Symbola

! scroll
URxvt.saveLines: 999999
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true

! scrollback buffer
URxvt.secondaryScreen: true
URxvt.secondaryScroll: false
URxvt.secondaryWheel: true

! cursor/bar
URxvt.cursorBlink: true
URxvt.scrollBar: false

!! perl extensions
URxvt.perl-ext-common: default,matcher

! clickable urls
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.url-select.underline: true

! disable default bindings for control-shift
URxvt.iso14755: false
URxvt.iso14755_52: false

! copy paste
URxvt.keysym.Control-Shift-c: eval:selection_to_clipboard
URxvt.keysym.Control-Shift-C: eval:selection_to_clipboard
URxvt.keysym.Control-Shift-v: eval:paste_clipboard
URxvt.keysym.Control-Shift-V: eval:paste_clipboard
